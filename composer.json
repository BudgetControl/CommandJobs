{
    "name": "budgetcontrol/jobs",
    "description": "Jobs microservice",
    "type": "none",
    "license": "MIT",
    "repositories": {
        "budgetcontrol/libs-connector": {
            "type": "vcs",
            "url": "https://github.com/BudgetControl/Libs_BcConnector.git"
        },
        "budgetcontrol/library" : {
            "type":"vcs",
            "url":"https://github.com/BudgetControl/Library.git"
        },
        "budgetcontrol/schema-registry-service": {
            "type": "vcs",
            "url": "https://github.com/BudgetControl/Schema-Registry-Service.git"
        },
        "budgetcontrol/cryptable":{
            "type":"vcs",
            "url":"https://github.com/BudgetControl/LibsCryptable.git"
