        "budgetcontrol/library": "~v1.5",
        "budgetcontrol/schema-registry-service": "~v1.5.1",
        "budgetcontrol/connector": "~v2.0.3-beta",
        "bref/bref": "~2.4.1",
        "donatello-za/rake-php-plus": "^2.0",
        "budgetcontrol/cryptable": "^0.2.0",
        "budgetcontrol/seeds": "^v0.3.2",
        "predis/predis": "^3.0",
        "illuminate/cache": "^11.45"
